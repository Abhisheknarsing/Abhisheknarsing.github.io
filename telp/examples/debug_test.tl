సృష్టించు స్నేహితులు = ["కవిత", "మోహన్", "జ్యోతి", "హరీశ్", "ప్రియ", "రాజు", "నవీన్", "చిత్ర", "సూర్య", "గీత"]
సృష్టించు జాబితా = [20000, 40000, 35000, 200000, 4000, 7000, 21000, 45000, 35000, 28000]

# సరిచేసిన కోడ్: 'చేయి' (while) బదులుగా 'ఉంటే' (if)
కొరకు (సృష్టించు i = 0; i < 10; i = i + 1) {
    ఉంటే (జాబితా[i] > 10000) {
        మాట్లాడు(స్నేహితులు[i] + " నా స్నేహితుడు")
    }
}
